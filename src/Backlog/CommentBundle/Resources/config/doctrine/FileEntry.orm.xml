<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity table="comment_entry_file" name="Backlog\CommentBundle\Entity\FileEntry">
        <field name="filename" type="string" length="256" />
        <field name="objectIdentifier" type="string" length="256" />
        <field name="mimeType" type="string" length="256" />
        <field name="description" type="text" nullable="true" />
        <field name="descriptionHtml" type="text" nullable="true" />
    </entity>

</doctrine-mapping>
