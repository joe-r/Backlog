<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="bl_app.form.backlog_type" class="Backlog\BacklogBundle\Form\Type\BacklogType">
            <tag name="form.type" alias="bl_backlog" />
        </service>

        <service id="bl_app.form.story_type" class="Backlog\BacklogBundle\Form\Type\StoryType">
            <tag name="form.type" alias="bl_story" />
        </service>

        <service id="bl_app.form.milestone_type" class="Backlog\BacklogBundle\Form\Type\MilestoneType">
            <tag name="form.type" alias="bl_milestone" />
        </service>
    </services>
</container>
